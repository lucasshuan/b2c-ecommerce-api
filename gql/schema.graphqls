type User {
  id: ID!
  name: String!
  email: String!
  password: String!
  createdAt: String!
  updatedAt: String!
}

type Query {
  getUsers: [User!]!
  getUser(id: ID!): User
}

input NewUser {
  name: String!
  email: String!
  password: String!
}

type Mutation {
  createUser(input: NewUser!): User!
}

schema {
  query: Query
  mutation: Mutation
}